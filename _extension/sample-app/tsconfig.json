{
  "compilerOptions": {
    "module": "esNext",
    "noImplicitAny": true,
    "removeComments": true,
    "moduleResolution": "Node",
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "jsx": "react"
  },
  "include": ["src/**/*", "src/**/*.json"],
  "exclude": ["node_modules"]
}
